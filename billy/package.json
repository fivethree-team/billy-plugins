{
    "name": "billy",
    "version": "0.0.1",
    "description": "cli plugin system personal assistant.",
    "main": "dist/generated/index.js",
    "scripts": {
        "build": "node_modules/.bin/tsc -p .",
        "start": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/generated/index.ts",
        "refresh": "rm -rf ./node_modules ./package-lock.json && npm install"
    },
    "preferGlobal": false,
    "keywords": [
        "typescript",
        "cli",
        "node",
        "terminal"
    ],
    "author": "garygrossgarten",
    "license": "MIT",
    "devDependencies": {
        "axios": "^0.18.0",
        "@types/node": "^10.12.18",
        "nodemon": "^1.18.9",
        "ts-node": "^7.0.1",
        "typescript": "^3.2.2",
        "typescript-ioc": "^1.2.4"
    },
    "dependencies": {
        "@fivethree/billy-core": "0.7.0",
        "@fivethree/billy-plugin-core": "0.7.0",
        "@fivethree/markdown": "file:../markdown"
    },
    "billy": {
        "plugins": [
            "@fivethree/billy-plugin-core",
            "@fivethree/markdown"
        ]
    }
}
