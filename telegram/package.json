{
    "name": "@fivethree/billy-plugin-telegram",
    "version": "0.0.3",
    "description": "example plugin for billy - your personal cli assistant.",
    "main": "dist/index.js",
    "scripts": {
        "build": "node_modules/.bin/tsc -p .",
        "start": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
        "refresh": "rm -rf ./node_modules ./package-lock.json && npm install"
    },
    "keywords": [
        "typescript",
        "cli",
        "node",
        "terminal"
    ],
    "author": "garygrossgarten",
    "license": "MIT",
    "devDependencies": {
        "axios": "^0.18.0",
        "@types/node": "10.12.29",
        "nodemon": "1.18.10",
        "ts-node": "7.0.1",
        "typescript": "3.3.3333"
    },
    "dependencies": {
        "node-telegram-bot-api": "^0.30.0",
        "telegraf": "^3.25.5",
        "@fivethree/billy-core": "0.9.2"
    }
}
